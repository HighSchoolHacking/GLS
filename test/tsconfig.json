{
    "compilerOptions": {
        "declaration": false,
        "module": "commonjs",
        "moduleResolution": "node",
        "target": "es5",
        "noFallthroughCasesInSwitch": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "outDir": ".",
        "pretty": true,
        "rootDir": ".",
        "sourceMap": true,
        "strictNullChecks": true
    },
    "include": [
        "./**/*.ts"
    ]
}
