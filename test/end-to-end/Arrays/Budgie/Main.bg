file start : Arrays Main
    main context start
        main start
            comment line : Initialization
            variable declare : aaa { array type : string } { array new : string }
            variable declare : bbb { array type : int } { array new : int 1 2 3 }
            variable declare : ccc { array type : { array type : string } } { array new : { array type : string } { variable : aaa } { array new : string "eee" "fff" "ggg" } }

            comment line : Members
            variable declare : fruits { array type : string } { array new : string "apple" "banana" "cherry" }
            print : { string format : ("The first fruit is {0}.") { array get : { variable : fruits } 0 } string }

            comment line : Length
            print : { string format : ("There are {0} fruits.") { array length : { variable : fruits } } int }
        main end
    main context end
file end
